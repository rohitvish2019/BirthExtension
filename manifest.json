{
  "manifest_version": 3,
  "name": "Data Fetcher",
  "version": "1.0",
  "description": "Fetch data from an API and use it on a website.",
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}